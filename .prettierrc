{
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "importOrder": [
        "^(react).*",
        "^@Utils/(.*)$",
        "^@Apis/(.*)$",
        "^@Hooks/(.*)$",
        "^@Contexts/(.*)$",
        "^@Scripts/(.*)$",
        "^@Pages/(.*)$",
        "^@Components/(.*)$",
        "^@Assets/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
